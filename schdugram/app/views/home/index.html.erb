                <h4>Calculate your monthly cost</h4>
                            <script>
                function calculateAmounts(){
                    var currency = $("#selectedCurrency").val();
                    $('.pricingrow').each(function(index,item){
                        if($(this).find(".followerselect").val() ==='lessthan10'){
                            if(currency === 'usd' || currency === 'aud'){
                                var amount = 20;
                            } else if (currency === 'gbp'){
                                var amount = 12;
                            }else if (currency === 'eur'){
                                var amount = 16;
                            }
                        } else if($(this).find(".followerselect").val() ==='lessthan100'){
                            if(currency === 'usd' || currency === 'aud'){
                                var amount = 30;
                            } else if (currency === 'gbp'){
                                var amount = 19;
                            }else if (currency === 'eur'){
                                var amount = 25;
                            }
                        } else if($(this).find(".followerselect").val()==='lessthan500'){
                            if(currency === 'usd' || currency === 'aud'){
                                var amount = 50;
                            } else if (currency === 'gbp'){
                                var amount = 32;
                            }else if (currency === 'eur'){
                                var amount = 41;
                            }
                        } else if($(this).find(".followerselect").val() === 'lessthan1m'){
                            if(currency === 'usd' || currency === 'aud'){
                                var amount = 70;
                            } else if (currency === 'gbp'){
                                var amount = 45;
                            }else if (currency === 'eur'){
                                var amount = 57;
                            }
                        } else if($(this).find(".followerselect").val() === 'lessthan2m'){
                            if(currency === 'usd' || currency === 'aud'){
                                var amount = 100;
                            } else if (currency === 'gbp'){
                                var amount = 65;
                            }else if (currency === 'eur'){
                                var amount = 88;
                            }
                        } else if($(this).find(".followerselect").val() === 'morethan2m'){
                            var amount = "Contact us";
                        }
                        amount = Math.round(amount);
                        var acctnumber = $('.pricingrow').length;
                        if(acctnumber<10){
                            var discount = 1;
                        }else if(acctnumber > 9 && acctnumber < 20){
                            discount = 0.8;
                        } else if (acctnumber >19 && acctnumber < 30){
                            discount = 0.75;
                        } else if (acctnumber >29 && acctnumber < 40){
                            discount = 0.7;
                        } else if (acctnumber > 39){
                            discount = '';
                        }
                        if(discount <1){
                            $(this).find(".acctprice").text(amount*discount);
                        }
                        $(this).find(".acctprice").text(Math.round(amount*discount));
                        $(this).find(".acctprice").val(Math.round(amount*discount));
                        if($(this).find(".acctprice").text()=="NaN"){
                            $(this).find(".acctprice").text("Contact us");
                        }
                        $('#accountNumber').text(acctnumber);
                        if(acctnumber>1){
                            $('#accountNumberText').text("accounts");
                        }
                    })
                    calculateTotals();
                }
                function calculateTotals(){
                    var total = 0;
                    $('.acctprice').each(function(index,item){
                        total = Number(total) + Number($(item).val());
                    });
                    if(isNaN(total)){
                        $('#totalcost').text("Contact us");
                        $('#totalannualcost').text("Contact us");
                    } else{
                        $('#totalcost').text(Math.round(total));
                        $('#totalannualcost').text((Math.round(total))*10);
                    }
                }
                function addRow(){
                    var template='<tr class="pricingrow"><td><select style="font-size:16px;" class="followerselect" onchange="calculateAmounts()"><option value="lessthan10">0 to 10,000 followers</option><option value="lessthan100">10,001 to 100,000 followers</option><option value="lessthan500">100,001 to 500,000 followers</option><option value="lessthan1m">500,001 to 1,000,000 followers</option><option value="lessthan2m">1,000,001 to 2,000,000 followers</option><option value="morethan2m">More than 2 million followers</option></select></td><td><span class="currency">$</span><span class="acctprice" value="20" >20</span></td></tr>';
                    $('#pricingcalculator').append(template);
                    changeCurrency();
                    calculateAmounts();
                }
                function changeCurrency() {
                    var currency = $("#selectedCurrency").val();
                    var sign = '$';
                    if(currency === 'usd' || currency === 'aud'){
                        sign = '$';
                    } else if (currency === 'eur'){
                        sign = '€';
                    } else if (currency === 'gbp'){
                        sign = '£';
                    }
                    $('.currency').text(sign);
                    calculateAmounts();
                }
            </script>
